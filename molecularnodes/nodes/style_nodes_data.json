        "max_value": null,
        "subtype": null,
        "enum_options": null
      },
      {
        "name": "Bond Find Scale",
        "type": "VALUE",
        "default_value": 1.0,
        "description": "Scale the VDW radii of the atoms when searching for bonds",
        "min_value": 0.0,
        "max_value": 10000.0,
        "subtype": "NONE",
        "enum_options": null
      },
      {
        "name": "Color Blur",
        "type": "BOOLEAN",
        "default_value": false,
        "description": "Interpolate between colors when enabled. When disabled the faces will take their color from their corresponding atom without interpolating",
        "min_value": null,
        "max_value": null,
        "subtype": null,
        "enum_options": null
      },
      {
        "name": "Shade Smooth",
        "type": "BOOLEAN",
        "default_value": true,
        "description": "Apply smooth shading to the created geometry",
        "min_value": null,
        "max_value": null,
        "subtype": null,
        "enum_options": null
      },
      {
        "name": "Material",
        "type": "MATERIAL",
        "default_value": null,
        "description": "Material to apply to the resulting geometry",
        "min_value": null,
        "max_value": null,
        "subtype": null,
        "enum_options": null
      }
    ]
